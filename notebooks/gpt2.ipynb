    "class FeedForward(nn.Module):\n",
    "    def __init__(self, emb_size):\n",
    "        super().__init__()\n",
    "        self.layer1 = nn.Linear(emb_size, 4*emb_size)\n",
    "        self.gelu = nn.GELU()\n",
    "        self.layer2 = nn.Linear(4*emb_size, emb_size)\n",
    "\n",
    "    def forward(self, x):\n",
    "        return self.layer2(self.gelu(self.layer1(x)))"
   ]
